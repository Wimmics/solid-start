<> a idx:Index;
  idx:hasShape [
      a sh:NodeShape;
      sh:property [
          sh:path rdf:type;
          sh:hasValue idx:Index
      ];
      sh:property [
        sh:path schema:hasLocation;
      ];  
  ].

:entry1 a idx:IndexEntry;
  sh:property [
    sh:hasValue "Paris"
  ];
  idx:hasTarget <https://storage.net/indexParis>.

:entry2 a idx:IndexEntry;
  sh:property [
    sh:hasValue "Toulouse"
  ];
  idx:hasTarget <https://storage.net/indexToulouse>.